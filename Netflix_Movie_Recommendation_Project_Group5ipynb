    "            \n",
    "]\n",
    "        \n",
    "my_ratings_df = sqlContext.createDataFrame(my_rated_movies, schema=testingRatings_df_schema)\n",
    "my_ratings_df.cache()\n",
    "seed = 1999\n",
    "my_train_df, my_test_df, my_val_df = my_ratings_df.randomSplit([6.0, 3.0, 1.0], seed)\n",
