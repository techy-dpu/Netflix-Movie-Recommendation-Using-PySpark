    "my_als = ALS(\n",
    "         rank=20, maxIter=10, regParam=0.05, \n",
    "         userCol=\"UserID\", \n",
    "         itemCol=\"MovieID\",\n",
    "         ratingCol=\"Rating\", \n",
    "         nonnegative = True, \n",
    "         implicitPrefs = False,\n",
    "         coldStartStrategy=\"drop\"\n",
    ")"
   ]
